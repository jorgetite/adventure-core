{
    "name": "jtite/adventure-core",
    "description": "A simple PHP framework to create text adventure games",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/jorgetite/adventure-core",
    "authors": [
      {
        "name": "Jorge Tite",
        "email": "jorgetite@me.com",
        "homepage": "http://www.jorgetite.com/"
      }
    ],
    "require": {
        "php": ">=7.1",
        "marc-mabe/php-enum" : "^1.0",
        "php-ds/php-ds" : "^1.1"
    },
    "require-dev": {
      "phpunit/phpunit": "^6.0"
    },
    "autoload": {
      "psr-4": {"Adventure\\": "src/adventure"}
    },
    "autoload-dev": {
      "psr-4": {"Adventure\\": "tests/adventure"}
    },
    "extra": {
      "branch-alias": {
        "dev-master": "1.0-dev"
      }
    }
}
